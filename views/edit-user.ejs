<body>

    <form method="POST"   class="border classForm p-4 col-4 m-2 ">
        <h3><%=title%></h3>
        <p class="text-success"><%=msg%></p>
        <table class="col-12">
               
            <tr>
                <td>First name:</td>
                <td>
                    <input type="text" class="form-control" name="firstName" placeholder="Enter name"
                        value="<%= user.firstName %>" required>
                </td>
            </tr>
            <tr>
                <td>Last name:</td>
                <td>
                    <input type="text" class="form-control" name="lastName" placeholder="Enter last name"
                        value="<%= user.lastName %>" required>
                </td>
            </tr>
            <tr>
                <td>username:</td>
                <td>
                    <input type="text" class="form-control" name="username" placeholder="Enter new username"
                        value="<%= user.username %>" required>
                </td>
            </tr>
            <tr>
                <td> gender:</td>
                <td>
                        <input type="text" class="form-control" name="gender" placeholder="Enter gender"
                        value="<%= user.gender %>" required>
                      
                </td>
            </tr>
            <tr>
                <td>password:</td>
                <td>
                    <input type="password" class="form-control" name="password" placeholder="Enter new password"
                        value="<%= user.password %>" required>
                </td>
            </tr>
         

            <tr>
                   <td>
                       <input class="btn btn-dark" id="sumbitEdit" type="submit">
                       <a class="btn btn-dark" href="/api/panel">Return</a>
                   </td>
                </tr>
        </table>
    </form>



